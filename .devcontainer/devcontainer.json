{
  "name": "scipro-container",
  "dockerFile": "Dockerfile",
  "context": ".",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "features": {
    "ghcr.io/devcontainers/features/python:1": {}
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "portsAttributes": {
    "8888": {
      "label": "JupyterLab",
      "onAutoForward": "openBrowser"
    }
  },
  "forwardPorts": [8888]
}
