{
  "name": "scipro-container",
  "dockerFile": "Dockerfile",
  "context": ".",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-python.python",
    "jupyter.jupyter-notebooks"
  ],
  "portsAttributes": {
    "8888": {
      "label": "JupyterLab",
      "onAutoForward": "openInBrowser"
    }
  }
}
