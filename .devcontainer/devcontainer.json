{
  "name": "scipro-container",
  "dockerFile": "Dockerfile",
  "context": ".",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "postCreateCommand": "pyflyby.install_in_ipython_config_file && jupyter lab --ip=0.0.0.0 --port=8888 --no-browser --NotebookApp.token=''",
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "portsAttributes": {
    "8888": {
      "label": "JupyterLab",
      "onAutoForward": "openBrowser"
    }
  },
  "forwardPorts": [8888]
}
