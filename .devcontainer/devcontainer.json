{
  "name": "scipro-container",
  "dockerFile": ".devcontainer/Dockerfile",
  "context": "..",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "portsAttributes": {
    "8888": {
      "label": "Jupyter",
      "onAutoForward": "openBrowser"
    }
  },
  "forwardPorts": [8888],
  "overrideCommand": false,
  "postCreateCommand": "pip install -r /tmp/requirements.txt"
}
